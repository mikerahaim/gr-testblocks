<?xml version="1.0"?>
<block>
  <name>interp_var</name>
  <key>testblocks_interp_var</key>
  <category>[testblocks]</category>
  <import>import testblocks</import>
  <make>testblocks.interp_var($interpolation)</make>
  
  <param>
    <name>Interpolation</name>
    <key>interpolation</key>
    <type>float</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
