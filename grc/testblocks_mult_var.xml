<?xml version="1.0"?>
<block>
  <name>mult_var</name>
  <key>testblocks_mult_var</key>
  <category>testblocks</category>
  <import>import testblocks</import>
  <make>testblocks.mult_var($mult)</make>
  <callback>set_mult($mult)</callback>
  
  <param>
    <name>Multiply Value</name>
    <key>mult</key>
    <type>float</type>
  </param>

  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
