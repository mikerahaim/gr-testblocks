<?xml version="1.0"?>
<block>
  <name>streams_to_streams_ff</name>
  <key>testblocks_streams_to_streams_ff</key>
  <category>[testblocks]/Multiple IOs</category>
  <import>import testblocks</import>
  <make>testblocks.streams_to_streams_ff($num_streams)</make>
  <param>
    <name>Number of Streams</name>
    <key>num_streams</key>
    <type>int</type>
  </param>
  
  <sink>
     <name>in</name>
     <type>float</type>
     <nports>$num_streams</nports>
  </sink>
   <source>
     <name>out</name>
     <type>float</type>
     <nports>$num_streams</nports>
  </source>
  
  
</block>
